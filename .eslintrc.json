{
  "extends": [
    "eslint:recommended",
    "eslint-config-next",
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "jsx-a11y/alt-text": "off"
  },
  "parser": "@typescript-eslint/parser",
  "ignorePatterns": [
    "node_modules/*",
    "out/*",
    ".next/*",
    ".vscode/*",
    ".husky/*"
  ]
}
